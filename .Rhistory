# Load necessary libraries
library(stats)
library(caret)
# Standardize the data
scaled_data <- scale(df_2022[, c('Station1','Station2',
'Station3','Station4','Station5',
'Station6','Station7')])
# Perform PCA
pca_result <- prcomp(na.omit(scaled_data), scale. = TRUE)
# Select number of components to retain (e.g., based on variance explained)
# You can use the summary(pca_result) to see the variance explained by each component
num_components <- 2
# Transform data using selected components
pcs_data <- data.frame(predict(pca_result, scaled_data)[, 1:num_components])
pca_data=cbind(df_2022$Interview,pcs_data$PC1, pcs_data$PC2)
colnames(pca_data)=c('Interview','PC1', 'PC2')
pca_data[is.na(pca_data)] <- 0
pca_data=data.frame(pca_data)
# Fit regression model
model <- glm(Interview ~. , data =pca_data, family = binomial)
# Summary of the model
summary(model)
